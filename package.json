{
  "name": "@tadashi/jwt",
  "version": "7.0.0",
  "description": "Sign, verify and parse a JSON Web Token",
  "type": "module",
  "main": "src/jwt.js",
  "files": [
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "pretest": "xo",
    "test": "c8 --reporter=text --reporter=text-summary --reporter=lcov ava --tap"
  },
  "keywords": [
    "jwt",
    "sign",
    "verify",
    "parse"
  ],
  "author": "Thiago Lagden",
  "license": "MIT",
  "engines": {
    "node": ">=14.16.0"
  },
  "dependencies": {
    "@tadashi/hex-id": "2.0.0",
    "debug": "4.3.2",
    "jose": "2.0.5"
  },
  "devDependencies": {
    "@tadashi/sleep": "2.0.1",
    "ava": "3.15.0",
    "c8": "7.8.0",
    "xo": "0.44.0"
  },
  "xo": {
    "ignore": [
      "bin/**/*",
      "lab.js"
    ],
    "envs": [
      "node"
    ],
    "semicolon": false,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "capitalized-comments": 0,
      "unicorn/prevent-abbreviations": 0
    }
  }
}
