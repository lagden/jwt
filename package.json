{
  "name": "@tadashi/jwt",
  "version": "3.0.0",
  "description": "Sign, verify and parse a JSON Web Token",
  "main": "index.js",
  "files": [
    "lib",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "report": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "npx xo",
    "test": "ava"
  },
  "keywords": [
    "jwt",
    "sign",
    "verify",
    "parse"
  ],
  "author": "Thiago Lagden",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.1"
  },
  "dependencies": {
    "@tadashi/hex-id": "1.1.0",
    "debug": "4.1.1",
    "jsrsasign": "8.0.12"
  },
  "devDependencies": {
    "ava": "2.3.0"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "semicolon": false,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "capitalized-comments": 0
    }
  }
}
