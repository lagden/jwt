{
  "name": "@tadashi/jwt",
  "version": "8.0.0",
  "description": "Sign, verify and parse a JSON Web Token",
  "type": "module",
  "main": "src/jwt.js",
  "files": [
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "eslint": "eslint --config .eslintrc.yml --ext .js --ignore-path .gitignore .",
    "pretest": "npm run eslint",
    "test": "env DEBUG=tadashi-jwt:* c8 --reporter=text --reporter=text-summary --reporter=lcov ava --tap"
  },
  "keywords": [
    "jwt",
    "sign",
    "verify",
    "parse"
  ],
  "author": "Thiago Lagden",
  "license": "MIT",
  "engines": {
    "node": ">=14.16.0"
  },
  "dependencies": {
    "@tadashi/hex-id": "2.0.0",
    "debug": "4.3.2",
    "jose": "3.15.5"
  },
  "devDependencies": {
    "@tadashi/sleep": "2.0.1",
    "@babel/core": "7.15.0",
    "@babel/eslint-parser": "7.15.0",
    "ava": "3.15.0",
    "c8": "7.8.0",
    "eslint": "7.32.0",
    "eslint-config-xo": "0.38.0",
    "eslint-plugin-unicorn": "35.0.0"
  },
  "babel": {},
  "ava": {
    "nodeArguments": [
      "--trace-warnings"
    ]
  }
}
